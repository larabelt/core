{
  "name": "larabelt/core",
  "description": "Core package for any LaraBelt app.",
  "keywords": [
    "cms",
    "laravel",
    "larabelt"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Matt Crowe",
      "email": "github.mpc@zym.me"
    },
    {
      "name": "Rob LaSota",
      "email": "rob.lasota@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1.0",
    "barryvdh/laravel-cors": "^0.9.2",
    "doctrine/dbal": "^2.5",
    "guzzlehttp/guzzle": ">=6.2.0",
    "kalnoy/nestedset": "^4.2",
    "kint-php/kint": "^2.0",
    "laravel/framework": ">=5.5.0",
    "laravelcollective/html": "5.4.1",
    "maatwebsite/excel": "~2.1.0",
    "riimu/kit-phpencoder": "^2.1",
    "rutorika/sortable": "3.4.*",
    "sabre/uri": "^2",
    "silber/bouncer": "v1.0.0-rc.1",
    "spatie/db-dumper": "^2.9"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^2.3",
    "barryvdh/laravel-ide-helper": "2.3.2",
    "rap2hpoutre/laravel-log-viewer": "^0.8.0"
  },
  "autoload": {
    "psr-4": {
      "Belt\\Core\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  }
}